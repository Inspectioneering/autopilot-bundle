{
  "name": "inspectioneering/autopilot-bundle",
  "type": "symfony-bundle",
  "description": "Symfony bundle to communicate with the Autopilothq.com API",
  "keywords": ["autopilot", "marketing", "automation", "api", "workflow"],
  "license": "MIT",
  "authors": [
    {
      "name": "Nick Schmoyer",
      "email": "nschmoyer@inspectioneering.com"
    }
  ],
  "require": {
    "php": ">=5.6",
    "picr/php-autopilothq": "^0.1.5",
    "symfony/dependency-injection": "~2.7|~3.4.10|^4.0.10",
    "symfony/config": "~2.7|~3.3|~4.0",
    "symfony/http-kernel": "~2.7|~3.3|~4.0"
  },
  "require-dev": {
    "symfony/phpunit-bridge": "^3.3|^4.0"
  },
  "autoload": {
    "psr-4": { "Inspectioneering\\AutopilotBundle\\": "" },
    "exclude-from-classmap": [
      "/Tests/"
    ]
  }
}